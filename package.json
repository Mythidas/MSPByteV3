{
  "name": "mspbytev3",
  "version": "0.0.1",
  "private": "true",
  "workspaces": [
    "apps/*",
    "packages/*",
    "backend/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@workspace/frontend",
    "dev:agent": "turbo dev --filter=@workspace/agents",
    "supabase:types": "bunx supabase gen types typescript --project-id bhyazlrcyuvbporjyefx --schema public,views > packages/shared/src/types/schema.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "packageManager": "bun@1.1.26",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "@workspace/ts-config": "workspace:*",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "tsc-alias": "^1.8.16"
  },
  "dependencies": {
    "@supabase/postgrest-js": "^2.93.3",
    "@supabase/supabase-js": "^2.93.3",
    "turbo": "^2.8.1",
    "typescript": "5.7.3"
  }
}
